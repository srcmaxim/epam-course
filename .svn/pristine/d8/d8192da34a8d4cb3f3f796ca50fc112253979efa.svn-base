package ua.nure.koval.SummaryTask3.entity;

public class Dosage {

    protected int value;
    protected String period;

    public Dosage(int value, String period) {
        this.value = value;
        this.period = period;
    }

    public Dosage() {
    }

    public int getValue() {
        return value;
    }

    public void setValue(int value) {
        this.value = value;
    }

    public String getPeriod() {
        return period;
    }

    public void setPeriod(String value) {
        this.period = value;
    }

    @Override
    public String toString() {
        return "Dosage{" +
                "value=" + value +
                ", period='" + period + '\'' +
                '}';
    }
}
